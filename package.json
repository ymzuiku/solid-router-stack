{
  "name": "solid-router-stack",
  "version": "0.0.1",
  "types": "lib/index.ts",
  "private": false,
  "exports": {
    "import": "./esm/index.js",
    "require": "./cjs/index.js"
  },
  "resolutions": {
    "react": "17.0.2"
  },
  "peerDependencies": {
    "react": ">=16.8.0"
  },
  "scripts": {
    "esm": "esbuild --define:process.env.NODE_ENV=\\\"production\\\" lib/index.ts --outdir=esm --target=es6  --bundle --external:solid --format=esm --minify --splitting",
    "cjs": "esbuild --define:process.env.NODE_ENV=\\\"production\\\" lib/index.ts --outdir=cjs --target=es6  --bundle --external:solid --format=cjs --minify",
    "build": "npm run cjs && npm run esm"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ymzuiku/gewu-packages.git"
  },
  "bugs": {
    "url": "git+https://github.com/ymzuiku/gewu-packages.git"
  },
  "homepage": "git+https://github.com/ymzuiku/gewu-packages.git",
  "dependencies": {
    "solid-js": "^1.4.8"
  }
}